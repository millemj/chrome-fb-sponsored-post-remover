{
	"manifest_version": 2,
	"name": "FBNewsfeed Sponsored Ad Remover",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"https://www.facebook.com/*"
			],
			"js": ["content.js"]
		}
	],
	"web_accessible_resources": [
		"script.js"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": {
			"19": "img/icon19.png",
			"38": "img/icon38.png"
		}
	},
	"icons": {
		 "16": "img/icon16.png",
		 "48": "img/icon48.png",
		"128": "img/icon128.png"
	}
	 //,
	// "background": {
	// 	"scripts": ["background.js"]
	// }
}